# Settings that the LAMMPS build will import when this package library is used

user-atc_SYSINC = -DMKL_ILP64 -I$(MKLROOT)/include -mmic
user-atc_SYSLIB =  ${MKLROOT}/lib/mic/libmkl_scalapack_ilp64.a -Wl,--start-group  ${MKLROOT}/lib/mic/libmkl_cdft_core.a ${MKLROOT}/lib/mic/libmkl_intel_ilp64.a ${MKLROOT}/lib/mic/libmkl_intel_thread.a ${MKLROOT}/lib/mic/libmkl_core.a ${MKLROOT}/lib/mic/libmkl_blacs_intelmpi_ilp64.a -Wl,--end-group -liomp5 -lpthread -lm
user-atc_SYSPATH = -L/opt/intel/mkl/lib/mic
